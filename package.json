{
  "name": "base_mini_app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "pod": "(cd ios && pod install)",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:16": "react-native run-ios --simulator=\"iPhone 16 Pro Max\"",
    "ios:se": "react-native run-ios --simulator=\"iPhone SE (3rd generation)\"",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "start:log": "react-native start --reset-cache --client-logs",
    "bundle_android": "react-native bundle --dev false --platform android --entry-file index.js --bundle-output build/main.android.jsbundle --assets-dest=build",
    "bundle_ios": "react-native bundle --dev false --platform ios --entry-file index.js --bundle-output build/main.jsbundle --assets-dest=build",
    "build_bundle": "mkdir -p build && rm -rf build/* && yarn bundle_ios && yarn bundle_android && cd build && zip -r Archive.zip ."
  },
